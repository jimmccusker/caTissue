<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.Container"
        table="CATISSUE_CONTAINER"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_CONTAINER_SEQ</param>
            </generator>
        </id>

        <property
            name="activityStatus"
            type="string"
            update="true"
            insert="true"
            column="ACTIVITY_STATUS"
            length="20"
        />

        <property
            name="barcode"
            type="string"
            update="true"
            insert="true"
            column="BARCODE"
            length="100"
        />

        <many-to-one
            name="capacity"
            class="edu.wustl.catissuecore.domain.Capacity"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="CAPACITY_ID"
        />

        <set
            name="children"
            table="CATISSUE_CONTAINER"
            lazy="false"
            inverse="false"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="PARENT_CONTAINER_ID"
              />

              <one-to-many
                  class="edu.wustl.catissuecore.domain.Container"
              />
        </set>

        <many-to-one
            name="parent"
            class="edu.wustl.catissuecore.domain.Container"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="PARENT_CONTAINER_ID"
        />

        <property
            name="comment"
            type="string"
            update="true"
            insert="true"
            column="COMMENT"
            length="2000"
        />

        <property
            name="full"
            type="boolean"
            update="true"
            insert="true"
            column="FULL"
        />

        <property
            name="name"
            type="string"
            update="true"
            insert="true"
            column="NAME"
            length="100"
        />

        <property
            name="positionDimensionOne"
            type="int"
            update="true"
            insert="true"
            column="POSITION_DIMENSION_ONE"
            length="30"
        />

        <property
            name="positionDimensionTwo"
            type="int"
            update="true"
            insert="true"
            column="POSITION_DIMENSION_TWO"
            length="30"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Container.xml
            containing the additional properties and place it in your merge dir.
        -->

        <joined-subclass
            name="edu.wustl.catissuecore.domain.SpecimenArray"
            table="CATISSUE_SPECIMEN_ARRAY"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />

        <many-to-one
            name="createdBy"
            class="edu.wustl.catissuecore.domain.User"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="CREATED_BY_ID"
        />

        <set
            name="specimenArrayContentCollection"
            table="CATISSUE_SPECIMEN_ARRAY_CONTENT"
            lazy="false"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >

              <key
                  column="SPECIMEN_ARRAY_ID"
              />

              <one-to-many
                  class="edu.wustl.catissuecore.domain.SpecimenArrayContent"
              />
        </set>

        <many-to-one
            name="specimenArrayType"
            class="edu.wustl.catissuecore.domain.SpecimenArrayType"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SPECIMEN_ARRAY_TYPE_ID"
        />

        <many-to-one
            name="storageContainer"
            class="edu.wustl.catissuecore.domain.StorageContainer"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="STORAGE_CONTAINER_ID"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.StorageContainer"
            table="CATISSUE_STORAGE_CONTAINER"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />

        <set
            name="collectionProtocolCollection"
            table="CATISSUE_STORAGE_CONT_COLL_PROT_REL"
            lazy="false"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >

              <key
                  column="STORAGE_CONTAINER_ID"
              />

              <many-to-many
                  class="edu.wustl.catissuecore.domain.CollectionProtocol"
                  column="COLLECTION_PROTOCOL_ID"
                  outer-join="auto"
              />

        </set>

        <set
            name="holdsSpecimenClassCollection"
            table="CATISSUE_STOR_CONT_SPEC_CLASS"
            lazy="false"
            inverse="false"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="STORAGE_CONTAINER_ID"
              />
			
			<element column="SPECIMEN_CLASS" type="string" length="50"/>
        </set>

        <set
            name="holdsStorageTypeCollection"
            table="CATISSUE_STOR_CONT_STOR_TYPE_REL"
            lazy="false"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >

              <key
                  column="STORAGE_CONTAINER_ID"
              />

              <many-to-many
                  class="edu.wustl.catissuecore.domain.StorageType"
                  column="STORAGE_TYPE_ID"
                  outer-join="auto"
              />

        </set>

        <many-to-one
            name="site"
            class="edu.wustl.catissuecore.domain.Site"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SITE_ID"
        />

        <property
            name="tempratureInCentigrade"
            type="double"
            update="true"
            insert="true"
            column="TEMPERATURE"
            length="30"
        />

        <many-to-one
            name="storageType"
            class="edu.wustl.catissuecore.domain.StorageType"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="STORAGE_TYPE_ID"
        />

        </joined-subclass>

    </class>

</hibernate-mapping>

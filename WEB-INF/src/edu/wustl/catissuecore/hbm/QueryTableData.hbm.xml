<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.QueryTableData"
        table="CATISSUE_QUERY_TABLE_DATA"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="identifier"
            column="TABLE_ID"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="displayName"
            type="string"
            update="true"
            insert="true"
            column="DISPLAY_NAME"
            length="50"
        />

        <property
            name="tableName"
            type="string"
            update="true"
            insert="true"
            column="TABLE_NAME"
            length="50"
        />

        <property
            name="aliasName"
            type="string"
            update="true"
            insert="true"
            column="ALIAS_NAME"
            length="50"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-QueryTableData.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

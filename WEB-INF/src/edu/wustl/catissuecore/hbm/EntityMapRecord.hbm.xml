<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.EntityMapRecord"
        table="CATISSUE_ENTITY_MAP_RECORD"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_ENTITY_MAP_RECORD_SEQ</param>
            </generator>
        </id>

        <property
            name="entityMapId"
            type="long"
            update="true"
            insert="true"
            column="ENTITY_MAP_ID"
            length="30"
        />
        
        <property
            name="staticEntityRecordId"
            type="long"
            update="true"
            insert="true"
            column="STATIC_ENTITY_RECORD_ID"
            length="30"
        />
        
        <property
            name="dynamicEntityRecordId"
            type="long"
            update="true"
            insert="true"
            column="DYNAMIC_ENTITY_RECORD_ID"
            length="30"
        />
        
                
       <property
            name="createdDate"
            type="date"
            update="true"
            insert="true"
            column="CREATED_DATE"
        />
        
         <property
            name="modifiedDate"
            type="date"
            update="true"
            insert="true"
            column="MODIFIED_DATE"
        />
        
        <property
            name="createdBy"
            type="string"
            update="true"
            insert="true"
            column="CREATED_BY"
        />
        
        <property
            name="linkStatus"
            type="string"
            update="true"
            insert="true"
            column="STATUS"
            length="10"
        />
        
        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-EntityMapRecord.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

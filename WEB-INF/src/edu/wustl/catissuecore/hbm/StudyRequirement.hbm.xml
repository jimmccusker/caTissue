<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.dataModel.domain.StudyRequirement"
        table="CATISSUE_STUDY_REQUIREMENT"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="identifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="bioSpecimenType"
            type="string"
            update="true"
            insert="true"
            column="TYPE"
            length="50"
        />

        <property
            name="bioSpecimenSubType"
            type="string"
            update="true"
            insert="true"
            column="BIOSPECIMEN_SUB_TYPE"
            length="50"
        />

        <property
            name="quantity"
            type="double"
            update="true"
            insert="true"
            column="QUANTITY"
            length="50"
        />

        <property
            name="quantityUnit"
            type="string"
            update="true"
            insert="true"
            column="QUANTITY_UNIT"
            length="50"
        />

        <property
            name="diseaseStatus"
            type="string"
            update="true"
            insert="true"
            column="DISEASE_STATUS"
            length="50"
        />

        <many-to-one
            name="study"
            class="edu.wustl.catissuecore.dataModel.domain.Study"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="STUDY_IDENTIFIER"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-StudyRequirement.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.dataModel.domain.Sample"
        table="CATISSUE_SAMPLE"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="identifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="creationDate"
            type="date"
            update="true"
            insert="true"
            column="CREATION_DATE"
            length="50"
        />

        <many-to-one
            name="createdBy"
            class="edu.wustl.catissuecore.dataModel.domain.User"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="CREATED_BY"
        />

        <property
            name="type"
            type="string"
            update="true"
            insert="true"
            column="TYPE"
            length="50"
        />

        <property
            name="quality"
            type="string"
            update="true"
            insert="true"
            column="QUALITY"
            length="50"
        />

        <property
            name="gelImageURL"
            type="string"
            update="true"
            insert="true"
            column="GEL_IMAGE_URL"
            length="100"
        />

        <property
            name="laneNumber"
            type="string"
            update="true"
            insert="true"
            column="LANE_NUMBER"
            length="50"
        />

        <property
            name="quantity"
            type="double"
            update="true"
            insert="true"
            column="QUANTITY"
            length="50"
        />

        <property
            name="quantityUnit"
            type="string"
            update="true"
            insert="true"
            column="QUANTITY_UNIT"
            length="50"
        />

        <property
            name="concentration"
            type="double"
            update="true"
            insert="true"
            column="CONCENTRATION"
            length="50"
        />

        <property
            name="available"
            type="boolean"
            update="true"
            insert="true"
            column="AVAILABLE"
            length="50"
        />

        <property
            name="availableQuantity"
            type="double"
            update="true"
            insert="true"
            column="AVAILABLE_QUANTITY"
            length="50"
        />

        <many-to-one
            name="activityStatus"
            class="edu.wustl.catissuecore.dataModel.domain.ActivityStatus"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="ACTIVITY_STATUS_ID"
        />

        <property
            name="barcode"
            type="string"
            update="true"
            insert="true"
            column="BARCODE"
            length="50"
        />

        <many-to-one
            name="segment"
            class="edu.wustl.catissuecore.dataModel.domain.Segment"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SEGMENT_ID"
        />

        <many-to-one
            name="specimen"
            class="edu.wustl.catissuecore.dataModel.domain.Specimen"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SPECIMEN_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Sample.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

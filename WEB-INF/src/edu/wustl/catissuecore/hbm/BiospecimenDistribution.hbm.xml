<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.dataModel.domain.BiospecimenDistribution"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="identifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="biospecimenType"
            type="string"
            update="true"
            insert="true"
            column="BIOSPECIMEN_TYPE"
            length="50"
        />

        <property
            name="bioSpecimenIdentifier"
            type="long"
            update="true"
            insert="true"
            column="BIOSPECIMEN_IDENTIFIER"
            length="50"
        />

        <property
            name="quantity"
            type="double"
            update="true"
            insert="true"
            column="QUANTITY"
            length="50"
        />

        <property
            name="quanityUnit"
            type="string"
            update="true"
            insert="true"
            column="QUANITY_UNIT"
            length="50"
        />

        <many-to-one
            name="distribution"
            class="edu.wustl.catissuecore.dataModel.domain.Distribution"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="DISTRIBUTION_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-BiospecimenDistribution.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.CancerResearchGroup"
        table="CATISSUE_CANCER_RESEARCH_GROUP"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="systemIdentifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_CANCER_RES_GRP_SEQ</param>
            </generator>
        </id>

        <property
            name="name"
            type="string"
            update="true"
            insert="true"
            column="NAME"
            length="50"
            not-null="true"
            unique="true"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-CancerResearchGroup.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

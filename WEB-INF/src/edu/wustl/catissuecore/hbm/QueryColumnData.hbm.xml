<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.QueryColumnData"
        table="CATISSUE_QUERY_INTERFACE_COLUMN_DATA"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="identifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
            </generator>
        </id>

        <many-to-one
            name="tableData"
            class="edu.wustl.catissuecore.domain.QueryTableData"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="TABLE_ID"
        />

        <property
            name="columnName"
            type="string"
            update="true"
            insert="true"
            column="COLUMN_NAME"
            length="50"
        />

        <property
            name="displayName"
            type="string"
            update="true"
            insert="true"
            column="DISPLAY_NAME"
            length="50"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-QueryColumnData.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

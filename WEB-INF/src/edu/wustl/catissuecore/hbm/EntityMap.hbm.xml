<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.EntityMap"
        table="CATISSUE_ENTITY_MAP"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_ENTITY_MAP_SEQ</param>
            </generator>
        </id>

        <property
            name="containerId"
            type="long"
            update="true"
            insert="true"
            column="CONTAINER_ID"
            length="30"
        />
        
        <property
            name="staticRecordId"
            type="long"
            update="true"
            insert="true"
            column="STATIC_RECORD_ID"
            length="30"
        />
        
        <property
            name="staticEntityId"
            type="long"
            update="true"
            insert="true"
            column="STATIC_ENTITY_ID"
            length="30"
        />
        
        <property
            name="createdDate"
            type="date"
            update="true"
            insert="true"
            column="CREATED_DATE"
        />
        
        <property
            name="createdBy"
            type="string"
            update="true"
            insert="true"
            column="CREATED_BY"
        />
        
        <property
            name="linkStatus"
            type="string"
            update="true"
            insert="true"
            column="STATUS"
            length="10"
        />
        
        <property
            name="typeId"
            type="long"
            update="true"
            insert="true"
            column="TYPE_ID"
            length="30"
        />

		
        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-EntityMap.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

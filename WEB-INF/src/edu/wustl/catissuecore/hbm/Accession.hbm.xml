<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.dataModel.domain.Accession"
        table="CATISSUE_ACCESSION"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="identifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="protocolNumber"
            type="long"
            update="true"
            insert="true"
            column="PROTOCOL_NUMBER"
            length="50"
        />

        <property
            name="medicalRecordNumber"
            type="string"
            update="true"
            insert="true"
            column="MEDICAL_RECORD_NUMBER"
            length="50"
        />

        <property
            name="medicalReportURL"
            type="string"
            update="true"
            insert="true"
            column="MEDICAL_REPORT_URL"
            length="100"
        />

        <property
            name="clinicalDiagnosis"
            type="string"
            update="true"
            insert="true"
            column="CLINICAL_DIAGNOSIS"
            length="50"
        />

        <property
            name="diseaseStatus"
            type="string"
            update="true"
            insert="true"
            column="DISEASE_STATUS"
            length="50"
        />

        <property
            name="surgicalPathologyNumber"
            type="string"
            update="true"
            insert="true"
            column="SURGICAL_PATHOLOGY_NUMBER"
            length="50"
        />

        <property
            name="surgicalPathologyReportURL"
            type="string"
            update="true"
            insert="true"
            column="SURGICAL_PATHOLOGY_REPORT_URL"
            length="100"
        />

        <property
            name="collectionTime"
            type="date"
            update="true"
            insert="true"
            column="COLLECTION_TIME"
            length="50"
        />

        <many-to-one
            name="collectedBy"
            class="edu.wustl.catissuecore.dataModel.domain.User"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="COLLECTED_BY"
        />

        <property
            name="receivedMode"
            type="string"
            update="true"
            insert="true"
            column="RECEIVED_MODE"
            length="50"
        />

        <property
            name="receivedTrackNumber"
            type="string"
            update="true"
            insert="true"
            column="RECEIVED_TRACK_NUMBER"
            length="50"
        />

        <property
            name="receivedTime"
            type="date"
            update="true"
            insert="true"
            column="RECEIVED_TIME"
            length="50"
        />

        <many-to-one
            name="receivedBy"
            class="edu.wustl.catissuecore.dataModel.domain.User"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="RECEIVED_BY"
        />

        <many-to-one
            name="activityStatus"
            class="edu.wustl.catissuecore.dataModel.domain.ActivityStatus"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="ACTIVITY_STATUS_IDENTIFIER"
        />

        <many-to-one
            name="participant"
            class="edu.wustl.catissuecore.dataModel.domain.Participant"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="PARTICIPANT_IDENTIFIER"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Accession.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

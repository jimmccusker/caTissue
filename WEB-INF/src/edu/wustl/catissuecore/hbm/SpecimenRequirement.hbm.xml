<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.SpecimenRequirement"
        table="CATISSUE_SPECIMEN_REQUIREMENT"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="systemIdentifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_SPECIMEN_REQ_SEQ</param>
            </generator>
        </id>

        <discriminator
            column="SPECIMEN_CLASS"
            type="string"
        />

        <property
            name="specimenType"
            type="string"
            update="true"
            insert="true"
            column="SPECIMEN_TYPE"
            length="50"
        />

        <property
            name="tissueSite"
            type="string"
            update="true"
            insert="true"
            column="TISSUE_SITE"
            length="150"
        />

        <property
            name="pathologyStatus"
            type="string"
            update="true"
            insert="true"
            column="PATHOLOGY_STATUS"
            length="50"
        />

        <set
            name="distributionProtocolCollection"
            table="CATISSUE_DISTRIBUTION_SPE_REQ"
            lazy="false"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="SPECIMEN_REQUIREMENT_ID"
              />

              <many-to-many
                  class="edu.wustl.catissuecore.domain.DistributionProtocol"
                  column="DISTRIBUTION_PROTOCOL_ID"
                  outer-join="auto"
              />

        </set>

        <set
            name="collectionProtocolEventCollection"
            table="CATISSUE_COLL_SPECIMEN_REQ"
            lazy="false"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="SPECIMEN_REQUIREMENT_ID"
              />

              <many-to-many
                  class="edu.wustl.catissuecore.domain.CollectionProtocolEvent"
                  column="COLLECTION_PROTOCOL_EVENT_ID"
                  outer-join="auto"
              />

        </set>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SpecimenRequirement.xml
            containing the additional properties and place it in your merge dir.
        -->
        <subclass
            name="edu.wustl.catissuecore.domain.FluidSpecimenRequirement"
            dynamic-update="false"
            dynamic-insert="false"
            discriminator-value="Fluid"
        >
        <property
            name="quantityInMilliliter"
            type="double"
            update="true"
            insert="true"
            column="QUANTITY"
            length="50"
        />

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-FluidSpecimenRequirement.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>
        <subclass
            name="edu.wustl.catissuecore.domain.MolecularSpecimenRequirement"
            dynamic-update="false"
            dynamic-insert="false"
            discriminator-value="Molecular"
        >
        <property
            name="quantityInMicrogram"
            type="double"
            update="true"
            insert="true"
            column="QUANTITY"
            length="50"
        />

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-MolecularSpecimenRequirement.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>
        <subclass
            name="edu.wustl.catissuecore.domain.CellSpecimenRequirement"
            dynamic-update="false"
            dynamic-insert="false"
            discriminator-value="Cell"
        >
        <property
            name="quantityInCellCount"
            type="int"
            update="true"
            insert="true"
            column="QUANTITY"
            length="50"
        />

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-CellSpecimenRequirement.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>
        <subclass
            name="edu.wustl.catissuecore.domain.TissueSpecimenRequirement"
            dynamic-update="false"
            dynamic-insert="false"
            discriminator-value="Tissue"
        >
        <property
            name="quantityInGram"
            type="double"
            update="true"
            insert="true"
            column="QUANTITY"
            length="50"
        />

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-TissueSpecimenRequirement.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>

    </class>

</hibernate-mapping>

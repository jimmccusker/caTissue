<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
>
    <class
        name="edu.wustl.catissuecore.domain.SpecimenRequirement"
        table="CATISSUE_SPECIMEN_REQUIREMENT"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_SPECIMEN_REQ_SEQ</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-SpecimenRequirement.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="specimenType"
            type="string"
            update="true"
            insert="true"
            column="SPECIMEN_TYPE"
            length="50"
        />

        <property
            name="tissueSite"
            type="string"
            update="true"
            insert="true"
            column="TISSUE_SITE"
            length="150"
        />

        <property
            name="pathologyStatus"
            type="string"
            update="true"
            insert="true"
            column="PATHOLOGY_STATUS"
            length="50"
        />

        <set
            name="distributionProtocolCollection"
            table="CATISSUE_DISTRIBUTION_SPE_REQ"
            lazy="false"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

            <key
                column="SPECIMEN_REQUIREMENT_ID"
            >
            </key>

            <many-to-many
                class="edu.wustl.catissuecore.domain.DistributionProtocol"
                column="DISTRIBUTION_PROTOCOL_ID"
                outer-join="auto"
             />

        </set>

        <set
            name="collectionProtocolEventCollection"
            table="CATISSUE_COLL_SPECIMEN_REQ"
            lazy="false"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

            <key
                column="SPECIMEN_REQUIREMENT_ID"
            >
            </key>

            <many-to-many
                class="edu.wustl.catissuecore.domain.CollectionProtocolEvent"
                column="COLLECTION_PROTOCOL_EVENT_ID"
                outer-join="auto"
             />

        </set>

        <many-to-one
            name="quantity"
            class="edu.wustl.catissuecore.domain.Quantity"
            cascade="save-update"
            outer-join="auto"
            update="true"
            insert="true"
            column="QUANTITY_ID"
        />

        <property
            name="specimenClass"
            type="string"
            update="true"
            insert="true"
            column="SPECIMEN_CLASS"
            length="50"
        />
    </class>

</hibernate-mapping>

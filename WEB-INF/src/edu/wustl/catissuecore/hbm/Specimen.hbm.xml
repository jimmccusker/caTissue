<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.Specimen"
        table="CATISSUE_SPECIMEN"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="systemIdentifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="type"
            type="string"
            update="true"
            insert="true"
            column="TYPE"
            length="50"
        />

        <property
            name="available"
            type="boolean"
            update="true"
            insert="true"
            column="AVAILABLE"
        />

        <property
            name="positionDimensionOne"
            type="int"
            update="true"
            insert="true"
            column="POSITION_DIMENSION_ONE"
            length="30"
        />

        <property
            name="positionDimensionTwo"
            type="int"
            update="true"
            insert="true"
            column="POSITION_DIMENSION_TWO"
            length="50"
        />

        <property
            name="barcode"
            type="string"
            update="true"
            insert="true"
            column="BARCODE"
            length="50"
        />

        <property
            name="comments"
            type="string"
            update="true"
            insert="true"
            column="COMMENTS"
            length="200"
        />

        <property
            name="activityStatus"
            type="string"
            update="true"
            insert="true"
            column="ACTIVITY_STATUS"
            length="50"
        />

        <many-to-one
            name="parentSpecimen"
            class="edu.wustl.catissuecore.domain.Specimen"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="PARENT_SPECIMEN_ID"
        />

        <set
            name="biohazardCollection"
            table="CATISSUE_SPECIMEN_BIOHAZARD_RELATIONSHIP"
            lazy="false"
            inverse="false"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="SPECIMEN_ID"
              />

              <many-to-many
                  class="edu.wustl.catissuecore.domain.Biohazard"
                  column="BIOHAZARD_ID"
                  outer-join="auto"
              />

        </set>

        <set
            name="specimenEventCollection"
            table="CATISSUE_SPECIMEN_EVENT"
            lazy="false"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="SPECIMEN_ID"
              />

              <one-to-many
                  class="edu.wustl.catissuecore.domain.SpecimenEventParameters"
              />
        </set>

        <set
            name="childrenSpecimen"
            table="CATISSUE_SPECIMEN"
            lazy="false"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="PARENT_SPECIMEN_ID"
              />

              <one-to-many
                  class="edu.wustl.catissuecore.domain.Specimen"
              />
        </set>

        <many-to-one
            name="storageContainer"
            class="edu.wustl.catissuecore.domain.StorageContainer"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="STORAGE_CONTAINER_IDENTIFIER"
        />

        <set
            name="externalIdentifierCollection"
            table="CATISSUE_EXTERNAL_IDENTIFIER"
            lazy="false"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="SPECIMEN_ID"
              />

              <one-to-many
                  class="edu.wustl.catissuecore.domain.ExternalIdentifier"
              />
        </set>

        <many-to-one
            name="specimenCollectionGroup"
            class="edu.wustl.catissuecore.domain.SpecimenCollectionGroup"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SPECIMEN_COLLECTION_GROUP_ID"
        />

        <many-to-one
            name="specimenCharacteristics"
            class="edu.wustl.catissuecore.domain.SpecimenCharacteristics"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SPECIMEN_CHARACTERISTICS_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Specimen.xml
            containing the additional properties and place it in your merge dir.
        -->

        <joined-subclass
            name="edu.wustl.catissuecore.domain.FluidSpecimen"
            table="CATISSUE_FLUID_SPECIMEN"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="quantityInMilliliter"
            type="double"
            update="true"
            insert="true"
            column="QUANTITY_IN_MILILITER"
            length="50"
        />

        <property
            name="availableQuantityInMilliliter"
            type="double"
            update="true"
            insert="true"
            column="AVAILABLE_QUANTITY_IN_MILILITER"
            length="50"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.CellSpecimen"
            table="CATISSUE_CELL_SPECIMEN"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="quantityInCellCount"
            type="int"
            update="true"
            insert="true"
            column="QUANTITY_IN_CELL_COUNT"
            length="50"
        />

        <property
            name="availableQuantityInCellCount"
            type="int"
            update="true"
            insert="true"
            column="AVAILABLE_QUANTITY_IN_CELL_COUNT"
            length="50"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.MolecularSpecimen"
            table="CATISSUE_MOLECULAR_SPECIMEN"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="concentrationInMicrogramPerMicroliter"
            type="double"
            update="true"
            insert="true"
            column="CONCENTRATION_IN_MICROGRAM_PER_MICROLITER"
            length="50"
        />

        <property
            name="quantityInMicrogram"
            type="double"
            update="true"
            insert="true"
            column="QUANTITY_IN_MICROGRAM"
            length="50"
        />

        <property
            name="availableQuantityInMicrogram"
            type="double"
            update="true"
            insert="true"
            column="AVAILABLE_QUANTITY_IN_MICROGRAM"
            length="50"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.wustl.catissuecore.domain.TissueSpecimen"
            table="CATISSUE_TISSUE_SPECIMEN"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="quantityInGram"
            type="double"
            update="true"
            insert="true"
            column="QUANTITY_IN_GRAM"
            length="50"
        />

        <property
            name="availableQuantityInGram"
            type="double"
            update="true"
            insert="true"
            column="AVAILABLE_QUANTITY_IN_GRAM"
            length="50"
        />

        </joined-subclass>

    </class>

</hibernate-mapping>

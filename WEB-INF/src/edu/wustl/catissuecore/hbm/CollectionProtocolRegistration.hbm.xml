<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.CollectionProtocolRegistration"
        table="CATISSUE_COLLECTION_PROTOCOL_REGISTRATION"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="systemIdentifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="protocolParticipantIdentifier"
            type="string"
            update="true"
            insert="true"
            column="PROTOCOL_PARTICIPANT_IDENTIFIER"
            length="50"
        />

        <property
            name="registrationDate"
            type="date"
            update="true"
            insert="true"
            column="REGISTRATION_DATE"
        />

        <many-to-one
            name="participant"
            class="edu.wustl.catissuecore.domain.Participant"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="PARTICIPANT_ID"
        />

        <many-to-one
            name="collectionProtocol"
            class="edu.wustl.catissuecore.domain.CollectionProtocol"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="COLLECTION_PROTOCOL_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-CollectionProtocolRegistration.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

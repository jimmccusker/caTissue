<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.StorageContainerCapacity"
        table="CATISSUE_STORAGE_CONT_CAPACITY"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="systemIdentifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_STRG_CONT_CAPA_SEQ</param>
            </generator>
        </id>

        <property
            name="oneDimensionCapacity"
            type="int"
            update="true"
            insert="true"
            column="ONE_DIMENSION_CAPACITY"
            length="30"
        />

        <property
            name="twoDimensionCapacity"
            type="int"
            update="true"
            insert="true"
            column="TWO_DIMENSION_CAPACITY"
            length="30"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-StorageContainerCapacity.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
>
    <class
        name="edu.wustl.catissuecore.domain.ClinicalReport"
        table="CATISSUE_CLINICAL_REPORT"
    >

        <id
            name="systemIdentifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_CLINICAL_REPORT_SEQ</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-ClinicalReport.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="surgicalPathologyNumber"
            type="string"
            update="true"
            insert="true"
            column="SURGICAL_PATHOLOGICAL_NUMBER"
            length="50"
        />

        <many-to-one
            name="participantMedicalIdentifier"
            class="edu.wustl.catissuecore.domain.ParticipantMedicalIdentifier"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="PARTICIPENT_MEDI_IDENTIFIER_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-ClinicalReport.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

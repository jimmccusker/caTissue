<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.dataModel.domain.BioSpecimenStorage"
        table="CATISSUE_BIOSPECIMEN_STORAGE"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="identifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="bioSpecimenType"
            type="string"
            update="true"
            insert="true"
            column="TYPE"
            length="50"
        />

        <property
            name="bioSpecimenIdentifier"
            type="long"
            update="true"
            insert="true"
            column="BIOSPECIMEN_IDENTIFIER"
            length="50"
        />

        <property
            name="positionDimensionOne"
            type="long"
            update="true"
            insert="true"
            column="POSITION_DIMENSION_ONE"
            length="50"
        />

        <property
            name="positionDimensionTwo"
            type="long"
            update="true"
            insert="true"
            column="POSITION_DIMENSION_TWO"
            length="50"
        />

        <property
            name="positionDimensionThree"
            type="long"
            update="true"
            insert="true"
            column="POSITION_DIMENSION_THREE"
            length="50"
        />

        <many-to-one
            name="box"
            class="edu.wustl.catissuecore.dataModel.domain.Box"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="BOX_IDENTIFIER"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-BioSpecimenStorage.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

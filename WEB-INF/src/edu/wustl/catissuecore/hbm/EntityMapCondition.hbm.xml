<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.EntityMapCondition"
        table="CATISSUE_ENTITY_MAP_CONDITIONS"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_ENTITY_MAP_CONDITION_SEQ</param>
            </generator>
        </id>

        <property
            name="staticRecordId"
            type="long"
            update="true"
            insert="true"
            column="STATIC_RECORD_ID"
            length="30"
        />

        <property
            name="typeId"
            type="long"
            update="true"
            insert="true"
            column="TYPE_ID"
            length="30"
        />

        <many-to-one
            name="entityMap"
            class="edu.wustl.catissuecore.domain.EntityMap"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="ENTITY_MAP_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-EntityMapCondition.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

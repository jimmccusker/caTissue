<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.catissuecore.domain.StorageType"
        table="CATISSUE_STORAGE_TYPE"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="systemIdentifier"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_STORAGE_TYPE_SEQ</param>
            </generator>
        </id>

        <property
            name="type"
            type="string"
            update="true"
            insert="true"
            column="TYPE"
            length="50"
            not-null="true"
            unique="true"
        />

        <property
            name="defaultTempratureInCentigrade"
            type="double"
            update="true"
            insert="true"
            column="DEFAULT_TEMP_IN_CENTIGRADE"
            length="30"
        />

        <property
            name="oneDimensionLabel"
            type="string"
            update="true"
            insert="true"
            column="ONE_DIMENSION_LABEL"
            length="50"
        />

        <property
            name="twoDimensionLabel"
            type="string"
            update="true"
            insert="true"
            column="TWO_DIMENSION_LABEL"
            length="50"
        />

        <property
            name="activityStatus"
            type="string"
            update="true"
            insert="true"
            column="ACTIVITY_STATUS"
            length="30"
        />

        <set
            name="storageTypeCollection"
            table="CATISSUE_STORAGETYPE_HOLDS_REL"
            lazy="false"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >

              <key
                  column="STORAGE_TYPE_ID"
              />

              <many-to-many
                  class="edu.wustl.catissuecore.domain.StorageType"
                  column="STORAGE_TYPE_HOLD_ID"
                  outer-join="auto"
              />

        </set>

        <set
            name="specimenClassCollection"
            table="CATISSUE_TYPE_SPECIMENCL_REL"
            lazy="false"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >

              <key
                  column="STORAGE_TYPE_ID"
              />

              <many-to-many
                  class="edu.wustl.catissuecore.domain.SpecimenClass"
                  column="SPECIMEN_CLASS_ID"
                  outer-join="auto"
              />

        </set>

        <set
            name="storageContainerCollection"
            table="CATISSUE_CONTAINER_TYPE_REL"
            lazy="false"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >

              <key
                  column="STORAGE_TYPE_ID"
              />

              <many-to-many
                  class="edu.wustl.catissuecore.domain.StorageContainer"
                  column="STORAGE_CONTAINER_ID"
                  outer-join="auto"
              />

        </set>

        <many-to-one
            name="defaultStorageCapacity"
            class="edu.wustl.catissuecore.domain.StorageContainerCapacity"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="STORAGE_CONTAINER_CAPACITY_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-StorageType.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

<ns1:ExtensionDescription extensionType="SERVICE" version="1.4" xmlns:ns1="gme://gov.nih.nci.cagrid.introduce/1/Extension" >
	<ns1:ServiceExtensionDescription displayName="Data Service" name="data">
		<ns1:CreationPostProcessor>gov.nih.nci.cagrid.data.creation.DataServiceQueryOperationProviderCreator</ns1:CreationPostProcessor>
		<ns1:CreationUIDialog>gov.nih.nci.cagrid.data.ui.creation.DataServiceCreationDialog</ns1:CreationUIDialog>
		<ns1:CodegenPreProcessor>gov.nih.nci.cagrid.data.codegen.DataServiceCodegenPreProcessor</ns1:CodegenPreProcessor>
		<ns1:CodegenPostProcessor>gov.nih.nci.cagrid.data.codegen.DataServiceOperationProviderCodegenPostProcessor</ns1:CodegenPostProcessor>
		<ns1:ServiceModificationUIPanel>gov.nih.nci.cagrid.data.ui.DataServiceModificationPanel</ns1:ServiceModificationUIPanel>
		<ns1:ServiceDeploymentUIPanel>gov.nih.nci.cagrid.data.ui.auditors.AuditorDeploymentConfigPanel</ns1:ServiceDeploymentUIPanel>
		<ns1:Properties>
			<ns1:Property makeGlobal="true" key="GME_URL" displayName="GME Service URL" description="Global Model Exchange URL" value="https://globalmodelexchange.training.cagrid.org:8443/wsrf/services/cagrid/GlobalModelExchange"/>
   			<ns1:Property makeGlobal="true" key="CADSR_DATA_URL" displayName="caDSR Data Service URL" description="Cancer Data Standards Repository (caDSR) Data Service URL" value="http://cadsr-dataservice.nci.nih.gov:80/wsrf/services/cagrid/CaDSRDataService"/>
            <ns1:Property makeGlobal="true" key="MMS_URL" displayName="MMS Service URL" description="Metadata Model Service (MMS) URL" value="http://mms.training.cagrid.org:8080/wsrf/services/cagrid/MetadataModelService"/>
		</ns1:Properties>
	</ns1:ServiceExtensionDescription>
	<ns1:UpgradesDescription>
		<ns1:UpgradeDescription upgradeClass="gov.nih.nci.cagrid.data.upgrades.DataServiceUpgradeFrom1pt2" fromVersion="1.2" toVersion="1.4"/>
		<ns1:UpgradeDescription upgradeClass="gov.nih.nci.cagrid.data.upgrades.DataServiceUpgradeFrom1pt3" fromVersion="1.3" toVersion="1.4"/>	
	</ns1:UpgradesDescription>
</ns1:ExtensionDescription>
